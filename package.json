{
  "name": "markbook",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8000/",
  "dependencies": {
    "@elastic/elasticsearch": "^8.1.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/chrome": "^0.0.179",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "add": "^2.0.6",
    "body-parser": "^1.19.2",
    "chai": "^4.3.6",
    "express": "^4.17.3",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "redis": "^4.0.4",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4",
    "webpack-cli": "^4.9.2"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "src/*"
      ],
      "extensions": "js,jsx,tsx",
      "delay": 2500,
      "runOnChangeOnly": true
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rimraf src/build",
    "server": "node src/server.js",
    "build": "npm-run-all build:*",
    "build:app": "INLINE_RUNTIME_CHUNK=false react-scripts build && mv build ./src/build",
    "build:bg": "webpack --mode production ./src/background.js --output-path ./src/build --output-filename background.js",
    "build:options": "webpack --mode production ./src/options.js --output-path ./src/build --output-filename options.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch": "npm-watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.24"
  }
}
