{
  "name": "Markbook",
  "description": "Content social media",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Markbook"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage",
    "history",
    "identity",
    "identity.email"
  ],
  "icons": {
    "16": "markbook-icon.png",
    "48": "markbook-icon.png",
    "128": "markbook-icon.png"
  }
}