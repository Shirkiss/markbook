{
  "name": "Markbook",
  "description": "Content social media",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Markbook"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage"],
  "icons": {
    "16": "markbook-icon.png",
    "48": "markbook-icon.png",
    "128": "markbook-icon.png"
  }
}